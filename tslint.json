{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard"
  ],
  "linterOptions": {
    "exclude": [
      "dist/*.d.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "case-insensitive"
      }
    ]
  },
  "rulesDirectory": []
}