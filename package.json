    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "vue": "^2.6.11",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "write-json-webpack-plugin": "^1.1.0"
